{
  "name": "vite-react-motoko",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "npm i && npm run setup:evm_rpc && npm run setup:backend",
    "setup:evm_rpc": "dfx deps pull && dfx deps init evm_rpc --argument '(record { nodesInSubnet = 28 })' && dfx deps deploy",
    "setup:backend": "dfx canister create backend && dfx generate backend && dfx deploy",
    "start": "run-p frontend backend",
    "frontend": "vite --port 3000 --host",
    "backend": "mo-dev --generate --deploy -y",
    "build": "tsc && vite build",
    "test": "run-s test:backend test:frontend",
    "test:frontend": "vitest run",
    "test:backend": "mo-test",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\" \"backend/**/*.mo\""
  },
  "dependencies": {
    "@dfinity/agent": "^1.3.0",
    "@dfinity/auth-client": "^1.3.0",
    "@dfinity/candid": "^1.3.0",
    "@dfinity/principal": "^1.3.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@hookform/resolvers": "^3.6.0",
    "@mui/icons-material": "^5.15.19",
    "@mui/material": "^5.15.19",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^5.40.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "date-fns": "^3.6.0",
    "embla-carousel-autoplay": "^8.1.5",
    "embla-carousel-react": "^8.1.5",
    "ethers": "^5.7.2",
    "lucide-react": "^0.390.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.5",
    "react-icons": "^5.2.1",
    "react-json-view-lite": "^1.2.1",
    "react-qr-code": "^2.0.15",
    "react-qr-scanner": "^1.0.0-alpha.11",
    "react-router-dom": "^6.23.1",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^0.9.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.0.1",
    "autoprefixer": "^10.4.19",
    "cross-fetch": "^3.1.6",
    "dotenv": "^16.3.1",
    "jsdom": "^22.1.0",
    "mo-dev": "^0.11.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^2.8.8",
    "prettier-plugin-motoko": "^0.5.3",
    "sass": "^1.63.6",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^0.32.2"
  }
}
